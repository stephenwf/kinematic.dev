{
  "compilerOptions": {
    "baseUrl": "src",
    "lib": ["dom", "es2020", "webworker"],
    "target": "es2018",
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "module": "CommonJS",
    "declaration": true,
    "declarationMap": true,
    "moduleResolution": "node",
    "outDir": "./lib",
    "noEmitOnError": false,
    "strict": true,
    "sourceMap": true,
    "skipLibCheck": true,
    "jsx": "react",
    "resolveJsonModule": true,
    "noImplicitAny": true,
    "downlevelIteration": true,
    "noUnusedLocals": false,
    "noFallthroughCasesInSwitch": true
  },
  "references": []
}
